shrooA=$(whoami)
shroober=chimken
shrooberUID=$(id -u $shroober)
shrooberGID=$(id -g $shroober)
shrooberHome=$(eval echo ~$shroober)
shrooberXRD=/run/user/$(id -u $shroober)
shrooVarsPath="$shrooPDir/script_res/exports/shrooVars"
shrooPDir=$shrooberHome/shroobada

shrooTraefikDir=/etc/traefik
shrooTraefikLogDir=/var/log/traefik

shrooAuthDir=/etc/authentik
shrooAuthDB=/var/lib/authdb

shrooGuacDir=/etc/guacamole
shrooGuacDB=/var/lib/guacdb
